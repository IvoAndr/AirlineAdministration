<section id="viewCatalog" class="background-img">
    <a href="#" class="add-flight">Add Flight +</a>
        <div class="added-flights">
            {{#each data}}
                <a href="#" class="added-flight" flightIndex="{{@index}}">
                    <img src="{{img}}" alt="" class="picture-added-flight">
                    <h3>{{destination}}</h3>
                    <span>from {{origin}}</span><span>{{departureDate}}</span>
                </a>
            {{/each}}
        </div>
</section>
